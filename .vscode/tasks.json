{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/D__GLIBCXX_BITSIZE_INT_N_0=128",
                "/D__GLIBCXX_TYPE_INT_N_0=__int128",
                "/Zi",
                "/EHsc",
                "/nologo",
                "/I${workspaceFolder}/include",
                "/I${workspaceFolder}/tests/unity/src",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${workspaceFolder}/src/*.c",
                "${workspaceFolder}/tests/unity/src/unity.c",
                "${file}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        },
        {
            "type": "shell",
            "label": "C: gcc.exe make debug exe",
            "command": "make",
            "args": [
                "${fileBasenameNoExtension}_debug"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "detail": "prelaunch for debugger"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc.exe build active file",
            "command": "C:/msys64/ucrt64/bin/gcc.exe",
            "args": [
                "-Itests/unity/src",
                "-Iinclude",
                "-g",
                "tests/unity/src/unity.c",
                "${workspaceFolder}/src/*.c",
                "${file}",
                "-o",
                "${workspaceFolder}/tests/debug.exe"
            ],
            "options": {
                "cwd": "C:/msys64/ucrt64/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build"
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}